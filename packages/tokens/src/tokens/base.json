{
  "color": {
    "white": {
      "$type": "color",
      "$value": "#fff"
    },
    "secondary900": {
      "$type": "color",
      "$value": "#5A287D"
    },
    "primary200": {
      "$type": "color",
      "$value": "#F8D2ED"
    },
    "primary900": {
      "$type": "color",
      "$value": "#DC1E83"
    },
    "extended": {
      "forest": {
        "$type": "color",
        "$value": "#11776D"
      },
      "cerise": {
        "$type": "color",
        "$value": "#BB1B74"
      }
    },
    "mono": {
      "black": {
        "$type": "color",
        "$value": "#2B2B2B"
      },
      "greyDark": {
        "$type": "color",
        "$value": "#6A7786"
      }
    }
  },
  "border-radius": {
    "pill": {
      "$type": "borderRadius",
      "$value": "30"
    }
  },
  "font-family": {
    "museo-sans": {
      "$type": "fontFamilies",
      "$value": "Museo Sans"
    },
    "museo-sans-rounded": {
      "$type": "fontFamilies",
      "$value": "Museo Sans Rounded"
    }
  },
  "text": {
    "button": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{font-family.museo-sans}",
        "fontWeight": "{font-weight.bold}",
        "fontSize": "14",
        "lineHeight": "20",
        "letterSpacing": "0.2",
        "paragraphSpacing": "0",
        "paragraphIndent": "0",
        "textDecoration": "none",
        "textCase": "initial"
      }
    }
  },
  "font-weight": {
    "bold": {
      "$type": "fontWeights",
      "$value": "700"
    }
  },
  "spacing": {
    "none": {
      "$type": "spacing",
      "$value": "0"
    },
    "base": {
      "$type": "spacing",
      "$value": "4"
    },
    "base-x-1": {
      "$type": "spacing",
      "$value": "{spacing.base}*1"
    },
    "base-x-2": {
      "$type": "spacing",
      "$value": "{spacing.base}*2"
    },
    "base-x-3": {
      "$type": "spacing",
      "$value": "{spacing.base}*3"
    },
    "base-x-4": {
      "$type": "spacing",
      "$value": "{spacing.base}*4"
    },
    "base-x-5": {
      "$type": "spacing",
      "$value": "{spacing.base}*5"
    },
    "base-x-6": {
      "$type": "spacing",
      "$value": "{spacing.base}*6"
    },
    "base-x-7": {
      "$type": "spacing",
      "$value": "{spacing.base}*7"
    },
    "base-x-8": {
      "$type": "spacing",
      "$value": "{spacing.base}*8"
    },
    "base-x-9": {
      "$type": "spacing",
      "$value": "{spacing.base}*9"
    }
  },
  "border": {
    "light": {
      "$type": "borderWidth",
      "$value": "0.5"
    },
    "regular": {
      "$type": "borderWidth",
      "$value": "1"
    },
    "semi-bold": {
      "$type": "borderWidth",
      "$value": "2"
    }
  },
  "helpers": {
    "text-align": {
      "left": {
        "$type": "other",
        "$value": "left"
      },
      "center": {
        "$type": "other",
        "$value": "center"
      },
      "right": {
        "$type": "other",
        "$value": "right"
      }
    },
    "keywords": {
      "unset": {
        "$type": "other",
        "$value": "unset"
      },
      "initial": {
        "$type": "other",
        "$value": "initial"
      },
      "revert": {
        "$type": "other",
        "$value": "revert"
      },
      "all": {
        "$type": "other",
        "$value": "all"
      },
      "none": {
        "$type": "other",
        "$value": "none"
      }
    }
  },
  "box-shadow": {
    "low": {
      "$type": "boxShadow",
      "$value": {
        "x": "0.263",
        "y": "6.995",
        "blur": "11",
        "spread": "0",
        "color": "rgba(0,0,0,0.16)",
        "type": "dropShadow"
      }
    }
  },
  "sizing": {
    "base": {
      "$type": "sizing",
      "$value": "4"
    },
    "base-x-14": {
      "$type": "sizing",
      "$value": "{sizing.base}*14"
    }
  }
}