{
  "name": "tokens-complete",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/tokens-complete/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nxkit/style-dictionary:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/tokens-complete",
        "styleDictionaryConfig": "packages/tokens-complete/style-dictionary.config.ts",
        "tsConfig": "packages/tokens-complete/tsconfig.json",
        "customActions": "packages/tokens-complete/src/extensions/actions/index.ts"
      }
    },
    "lint": {
      "command": "eslint packages/tokens-complete/**/*.{js,ts} packages/tokens-complete/package.json"
    }
  }
}
